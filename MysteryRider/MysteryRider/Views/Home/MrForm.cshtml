        <div class="section-input" id="1.18">
            <div class="form-group row">
                <label for="1.18-input" class="col-2 col-form-label">Surveyor Alighting Location: </label>
                <div class="col-md-4">
                    <input class="form-control" type="text" id="1.18-input">
                </div>
            </div>
        </div>
        <div class="section-input" id="1.19">
